# Backend Configuration
# Replace with your OpenAI API key (starts with "sk-").
OPENAI_API_KEY=sk-your-openai-key

# Frontend Configuration
# Backend API URL (for local dev use localhost:8001, for Docker use the service name)
VITE_API_BASE_URL=http://localhost:8001

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key

# YouTube Upload Protection
# Set this to a secret key to protect the upload endpoint
UPLOAD_API_KEY=your-secret-upload-key

# Langfuse Configuration (Observability)
# Set to "true" to enable Langfuse tracing
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key
LANGFUSE_BASE_URL=https://us.cloud.langfuse.com